{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(styles\.css|script\.js|demo-audio\.mp3|demo-audio\.txt)", "headers": { "cache-control": "public, max-age=31536000, immutable" } },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
